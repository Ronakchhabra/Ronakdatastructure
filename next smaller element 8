#include <stack>
#include <vector>
using namespace std;
vector<int> nextSmallerElement(vector<int> &arr, int n)
{
    stack<int> s;
    s.push(-1);
    vector<int> ans(n);

    for(int i= n-1 ; i>=0 ; i--)
    {
        int c2 = arr[i];

       
            while(s.top()>=c2)
            {
                s.pop();
            }
            ans[i]=s.top();
            s.push(c2);
        
    }
    return ans;
}
